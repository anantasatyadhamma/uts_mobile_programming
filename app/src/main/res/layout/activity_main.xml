<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btnCalculator"
        android:layout_width="120dp"
        android:layout_height="62dp"
        android:layout_marginStart="57dp"
        android:layout_marginLeft="57dp"
        android:layout_marginTop="108dp"
        android:background="#f4a261"
        android:text="Kalkulator"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Button
        android:id="@+id/btnList"
        android:layout_width="120dp"
        android:layout_height="62dp"
        android:layout_marginStart="59dp"
        android:layout_marginLeft="59dp"
        android:layout_marginTop="66dp"
        android:text="Berita Pendidikan"
        app:layout_constraintStart_toEndOf="@+id/btnBio"
        app:layout_constraintTop_toBottomOf="@+id/btnKeluar" />

    <Button
        android:id="@+id/btnBio"
        android:layout_width="120dp"
        android:layout_height="62dp"
        android:layout_marginStart="57dp"
        android:layout_marginLeft="57dp"
        android:layout_marginTop="65dp"
        android:text="Biodata"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCalculator" />

    <Button
        android:id="@+id/btnKeluar"
        android:layout_width="120dp"
        android:layout_height="62dp"
        android:layout_marginStart="59dp"
        android:layout_marginLeft="59dp"
        android:layout_marginTop="245dp"
        android:text="Keluar"
        app:layout_constraintStart_toEndOf="@+id/btnCalculator"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="154dp"
        android:layout_marginLeft="154dp"
        android:layout_marginTop="118dp"
        android:layout_marginEnd="175dp"
        android:layout_marginRight="175dp"
        android:text="DASHBOARD"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>